-- physics

function boxcol(x1,y1,x2,y2,w1,w2,h1,h2)
  w1 = w1 or 8
  h1 = h1 or 8
  w2 = w2 or 8
  h2 = h2 or 8
  
  if x1 < x2 + w2 and x2 < x1 + w1 and y1 < y2 + h2 and y2 < y1 + h1 then
   return true
  end
 return false

end


