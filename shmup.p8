-- ship
ship_spr = 17
muzzle = 0
bullet_pulse = 1

function init_ship()
 ship = {}
 ship.x = 64
 ship.y = 110
 ship.sx = 0
 ship.sy = 0
 ship.invul = 0
end

function update_ship()
 ship.sx *= 0.9
 if(btn(➡️)) ship.sx = 2
 if(btn(⬅️))	ship.sx = -2
 ship.x += ship.sx
 ship.x = max(ship.x, 0)
 ship.x = min(ship.x, 120)

 ship.sy *= 0.9
 if(btn(⬆️)) ship.sy = -2
 if(btn(⬇️))	ship.sy = 2
 ship.y += ship.sy
 ship.y = max(ship.y, 0)
 ship.y = min(ship.y, 112)
 
 
 if btn(❎) then
  if bullet_pulse < t() then
   add_bullet()
   sfx(0)
   muzzle = 5
   bullet_pulse = t() + 0.1
  end
 end
 
end

function draw_ship()
 if ship.sx > 1 then
  ship_spr=18
 elseif ship.sx < -1 then
  ship_spr=16
 else 
  ship_spr = 17
 end
 
 if ship.invul > t() then
  if flr(t()*10)%2 == 0 then
   ship_spr = 19
  end
 end
 spr(ship_spr, ship.x, ship.y)
 local b = flr(t()*24)%4+32
 spr(b, ship.x, ship.y+9)
 
 if muzzle > 0 then
  circfill(ship.x+3, ship.y-1, muzzle, 7)
  muzzle -= 1
 end
 
end
