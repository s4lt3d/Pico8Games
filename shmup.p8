-- stars

function init_stars()
	stars={}
	
	starpal={1,2,12,7}
	for i=1,100 do
 	local newstar = {}
		newstar.x=rnd(128)
		newstar.y=rnd(128)
		newstar.spd=rnd(4)+0.1
		add(stars, newstar)
	end
end

function draw_stars()
 for i=1,100 do
  local s = stars[i]
  
  local sc=flr(#starpal*s.spd/4)+1
  local c = starpal[sc]
  pset(s.x, s.y, c)
 end
end

function update_stars()
 for i=1,100 do
  local s = stars[i]
  s.y += s.spd
  if s.y > 128 then
   s.y -= 128
  end
 end
end
