function update_start()
 cls()
 update_stars()
 if btnp(❎) then
  mode = "game"
  reset_game()
  --costart(over_delay)
 end
end

function draw_start()
 draw_stars()
 printout("shmup", 25, 35, 8, 7)
 printout("press ❎ to start", 32, 80, 12,7)
end

function update_over()
 update_stars()
 if btnp(❎) then
  mode = "start"
 end
end

function draw_over()
 
 draw_stars()
 
 printout("game over", 34, 40, 3, 7)
 printout("press ❎", 52, 80, 8,7)
end

function printout(str,x,y,oc,fc)
 for ix =x-1,x+1 do
  for iy= y-1, y+1 do
   print(str, ix, iy, oc)
  end
 end
 print(str, x, y, fc)
end
