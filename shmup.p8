-- coroutines
coroutines = {}

function costart(co)
 local c = cocreate(co)
 add(coroutines, c)
 coresume(c)
end

function update_coroutines()
  for i = #coroutines, 1, -1 do
    local co = coroutines[i]
    if co and costatus(co) == 'dead' then
      del(coroutines, i)
    else
      coresume(co)
    end
  end
end

